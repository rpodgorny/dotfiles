"quodlibet --play-pause"
	Control + Alt + Home

"quodlibet --next"
	Control + Alt + End

"quodlibet-delete-current"
	Control + Alt + Delete + Insert

"amixer sset Master 5%-"
	Control + Alt + Next

"amixer sset Master 5%+"
	Control + Alt + Prior

"amixer sset Master toggle"
	XF86AudioMute

"amixer sset Master 5%-"
	XF86AudioLowerVolume

"amixer sset Master 5%+"
	XF86AudioRaiseVolume

"xbacklight -10"
	XF86MonBrightnessDown

"xbacklight +10"
	XF86MonBrightnessUp

"xinput set-prop 6 272 0"
	Control + Alt + m

"killall omxplayer.bin"
	Control + Alt + k
