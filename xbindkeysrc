"quodlibet --play-pause"
	Control + Alt + Home

"quodlibet --next"
	Control + Alt + End

"quodlibet-delete-current"
	Control + Alt + Delete + Insert

"amixer sset Master 5%-"
	Control + Alt + Next

"amixer sset Master 5%+"
	Control + Alt + Prior

"amixer sset Master toggle"
	XF86AudioMute

"amixer sset Master 1%-"
	XF86AudioLowerVolume

"amixer sset Master 1%+"
	XF86AudioRaiseVolume

"xbacklight -20"
	XF86MonBrightnessDown

"xbacklight +20"
	XF86MonBrightnessUp

"killall omxplayer.bin"
	Control + Alt + k
