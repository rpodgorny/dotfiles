ServerAliveInterval 10
ServerAliveCountMax 60

VerifyHostKeyDNS yes

Host aur.archlinux.org
	IdentityFile ~/.ssh/id_rsa_aur

Host bitbucket.org
	IdentityFile ~/.ssh/id_rsa_bitbucket
	User git

Host git.asterix.cz
	IdentityFile ~/.ssh/id_rsa_git_asterix_cz
	User git
	Port 2222

Host atxpkg.asterix.cz
	IdentityFile ~/.ssh/id_rsa_atxpkg
	User atxpkg

Host atxpkg-dev.asterix.cz
	IdentityFile ~/.ssh/id_rsa_atxpkg
	User atxpkg
