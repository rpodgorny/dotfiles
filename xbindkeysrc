"quodlibet --play-pause"
	Control + Alt + Home

"quodlibet --next"
	Control + Alt + End

"quodlibet-delete-current"
	Control + Alt + Delete + Insert

"amixer sset Master 5%-"
	Control + Alt + Next

"amixer sset Master 5%+"
	Control + Alt + Prior

"amixer sset Master toggle"
	XF86AudioMute

"amixer sset Master 2%-"
	XF86AudioLowerVolume

"amixer sset Master 2%+"
	XF86AudioRaiseVolume

#"xbacklight -5; notify-send $(xbacklight)"
#"xbacklight_helper dec 10; killall osd_cat; osd_cat -p middle -A center -b percentage -P $(xbacklight) -T $(xbacklight) -f '-misc-fixed-medium-r-semicondensed--30-*-*-*-c-*-*-*'"
"xbacklight_helper dec 10; notify-send '$(xbacklight)' -h int:value:$(xbacklight) -h string:synchronous:brightness -i notification-display-brightness"
	XF86MonBrightnessDown

#"xbacklight +5; notify-send $(xbacklight)"
#"xbacklight_helper inc 10; killall osd_cat; osd_cat -p middle -A center -b percentage -P $(xbacklight) -T $(xbacklight) -f '-misc-fixed-medium-r-semicondensed--30-*-*-*-c-*-*-*'"
"xbacklight_helper inc 10; notify-send '$(xbacklight)' -h int:value:$(xbacklight) -h string:synchronous:brightness -i notification-display-brightness"
	XF86MonBrightnessUp

"sudo asus-kbd-backlight down; notify-send $(asus-kbd-backlight show)"
	XF86KbdBrightnessDown

"sudo asus-kbd-backlight up; notify-send $(asus-kbd-backlight show)"
	XF86KbdBrightnessUp

"xinput set-prop 12 281 0"
	Control + Alt + m

"killall omxplayer.bin"
	Control + Alt + k
